class Solution:
    def frequencySort(self, s: str) -> str:
        heap = []
        res = Counter(s)
        for i,j in res.items():
            heapq.heappush(heap,(-j,i))
        ans = ""
        while heap:
            freq,key = heapq.heappop(heap)
            ans += (-freq) * key
        return ans
        

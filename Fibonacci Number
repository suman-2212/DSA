class Solution {
    public int fib(int n) {
        if (n == 0) return 0;
        if (n == 1) return 1;

        int last2 = 0;
        int last = 1;

        for (int i = 2; i <= n; i++) {
            int curr = last + last2;
            last2 = last;
            last = curr;
        }
        return last;
    }
}

class Solution:
    def maxArea(self, height: List[int]) -> int:
        a = 0
        l = 0
        r = len(height)-1
        while l<r:
            a = max(a, min(height[l], height[r])*(r-l))
            if height[l]>height[r]:
                r-=1
            else:
                l+=1
        return a

class Solution:
    def topKFrequent(self, nums: List[int], k: int) -> List[int]:
        heap = []
        ans = Counter(nums)

        for key,val in ans.items():
            heapq.heappush(heap,(val,key))
            if len(heap) > k:
                heapq.heappop(heap)
        res = []
        for item in heap:
            res.append(item[1])
        return res
